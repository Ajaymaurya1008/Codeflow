{
  "version": 2,
  "builds": [{ "src": "vite.config.js", "use": "@vercel/static-build" }],
  "routes": [{ "src": "/(.*)", "dest": "/" }],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
